@article{zhangGeneticControlIndividual2010,
  title = {Genetic {{Control}} of {{Individual Differences}} in {{Gene-Specific Methylation}} in {{Human Brain}}},
  author = {Zhang, Dandan and Cheng, Lijun and Badner, Judith A. and Chen, Chao and Chen, Qi and Luo, Wei and Craig, David W. and Redman, Margot and Gershon, Elliot S. and Liu, Chunyu},
  year = {2010},
