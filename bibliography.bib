@article{babikGenomicResponseUrbanization2023,
  title = {The Genomic Response to Urbanization in the Damselfly {{{\emph{Ischnura}}}}{\emph{ Elegans}}},
  author = {Babik, W. and Dudek, K. and Marsza{\l}ek, M. and Palomar, G. and Antunes, B. and Sniegula, S.},
