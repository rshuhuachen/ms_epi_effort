@article{sunGenomewideVariationDNA2021,
  title = {Genome-wide Variation in {{DNA}} Methylation Linked to Developmental Stage and Chromosomal Suppression of Recombination in White-throated Sparrows},
  author = {Sun, Dan and Layman, Thomas S. and Jeong, Hyeonsoo and Chatterjee, Paramita and Grogan, Kathleen and Merritt, Jennifer R. and Maney, Donna L. and Yi, Soojin V.},
