@article{valiWhatExtentMicrosatellite2008,
  title = {To What Extent Do Microsatellite Markers Reflect Genome-Wide Genetic Diversity in Natural Populations?},
  author = {V{\"a}li, {\"U}lo and Einarsson, Annika and Waits, Lisette and Ellegren, Hans},
  year = {2008},
