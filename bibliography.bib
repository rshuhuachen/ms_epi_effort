}

@article{pimpinelliEnvironmentalChangeEvolution2020,
  title = {Environmental Change and the Evolution of Genomes: {{Transposable}} Elements as Translators of Phenotypic Plasticity into Genotypic Variability},
  shorttitle = {Environmental Change and the Evolution of Genomes},
  author = {Pimpinelli, Sergio and Piacentini, Lucia},
  editor = {Herrel, Anthony},
  year = {2020},
  month = feb,
  journal = {Functional Ecology},
  volume = {34},
  number = {2},
  pages = {428--441},
  issn = {0269-8463, 1365-2435},
  doi = {10.1111/1365-2435.13497},
  urldate = {2024-03-21},
  abstract = {Abstract                                                                Phenotypic plasticity is generally explained as the result of epigenetic mechanisms modifying gene expression in response to changing environmental conditions. However, the biology of transposable elements (TEs) suggests that such elements may also induce differential gene expression by affecting regulatory regions.                                                     We discuss the ecological and evolutionary relevance of epigenetic modifications versus transposon activity, taking into account that epigenetic modifications are generally reversible but that the modifications induced by TEs are stably inherited.                                                     We outline our perspective on the multiple roles played by environmental changes in the context of adaptive evolution. Environmental perturbations can induce phenotypic variations via epigenetic modulation of gene expression and promote, at the same time, genetic variability by triggering bursts of TE activity; finally, they select which genetic variations are most advantageous for survival.                                                     Within this context, the production of environmentally induced advantageous phenotypes by epigenetic mechanisms could represent an immediate process of adaptation followed by TE-induced genotypic changes that make these phenotypic variants heritable through the germ line. This scenario could lead TEs to play different roles in the function of the time-scale of ecological variation, such as those related to climatic change in the current context of global change.                                                     In conclusion, we propose that through TE activation, environmental changes can act as inducers of genetic variability, upon which they also act as selective forces, thus triggering rapid evolutionary processes.                                                                         A free               Plain Language Summary               can be found within the Supporting Information of this article.                        ,              Abstract             La plasticit{\`a} fenotipica {\`e} definita come la capacit{\`a} di un singolo genotipo di esibire un ampio spettro di fenotipi in risposta alle mutevoli condizioni ambientali. Tale fenomeno {\`e} generalmente spiegato come il risultato di meccanismi epigenetici che modificano l'espressione genica. Tuttavia, la biologia degli elementi trasponibili suggerisce che tali elementi possano anche indurre un'espressione genica differenziale influenzando le regioni regolative. La rilevanza ecologica ed evolutiva delle modifiche epigenetiche rispetto all'attivit{\`a} degli elementi trasponibili sar{\`a} discussa tenendo conto del fatto che le modifiche epigenetiche sono generalmente reversibili mentre le modifiche indotte da tali elementi sono ereditate stabilmente. Questo scenario contempla lo svolgere di molteplici ruoli degli elementi trasponibili in funzione delle scale temporali di variazioni ecologiche come quelle relative al cambiamento climatico, un fattore importante nell'attuale contesto del cambiamento globale. Proponiamo che, attraverso l'attivazione degli elementi trasponibili, i cambiamenti ambientali fungano da induttori di variabilit{\`a} genetica sulla quale poi agiscono anche come forze selettive innescando cos{\`i} rapidi processi evolutivi.},
  langid = {english},
  keywords = {/unread},
  file = {/Users/vistor/Zotero/storage/ARJKKF2A/Pimpinelli and Piacentini - 2020 - Environmental change and the evolution of genomes Transposable elements as translators of phenotypi.pdf}
