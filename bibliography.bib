@article{perrierGreatTitsCity2018,
  title = {Great Tits and the City: {{Distribution}} of Genomic Diversity and Gene--Environment Associations along an Urbanization Gradient},
  shorttitle = {Great Tits and the City},
  author = {Perrier, Charles and Lozano Del Campo, Ana and Szulkin, Marta and Demeyrier, Virginie and Gregoire, Arnaud and Charmantier, Anne},
