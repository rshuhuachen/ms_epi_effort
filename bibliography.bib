@article{schmiedEffectPreviousStroking2010,
  title = {Effect of Previous Stroking on Reactions to a Veterinary Procedure},
  author = {Schmied, Claudia and Boivin, Xavier and Scala, Sebastian and Waiblinger, Susanne},
