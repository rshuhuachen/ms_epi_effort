@article{grieshopStrongSexualSelection2016,
  title = {Strong Sexual Selection in Males against a Mutation Load That Reduces Offspring Production in Seed Beetles},
  author = {Grieshop, K. and St{\aa}ngberg, J. and Martinossi-Allibert, I. and Arnqvist, G. and Berger, D.},
