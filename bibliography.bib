@article{rintamakiCorrelatesMaleMating2001,
  title = {Correlates of Male Mating Success on Black Grouse ({{Tetrao}} Tetrix {{L}}.) Leks},
  author = {Rintam{\"a}ki, Pekka T. and H{\"o}glund, Jacob and Alatalo, Rauno V. and Lundberg, Arne},
  year = {2001},
  journal = {Annales Zoologici Fennici},
