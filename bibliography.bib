@article{robinsonQuantifyingMultivariatePlasticity2013,
  title = {Quantifying Multivariate Plasticity: Genetic Variation in Resource Acquisition Drives Plasticity in Resource Allocation to Components of Life History},
  shorttitle = {Quantifying Multivariate Plasticity},
  author = {Robinson, Matthew R. and Beckerman, Andrew P.},
  editor = {Sorci, Gabriele},
