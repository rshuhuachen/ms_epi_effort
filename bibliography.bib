@article{teetsOverexpressionAntioxidantEnzyme2019,
  title = {Overexpression of an Antioxidant Enzyme Improves Male Mating Performance after Stress in a Lek-Mating Fruit Fly},
  author = {Teets, Nicholas M. and Dias, Vanessa S. and Pierce, Bailey K. and Schetelig, Marc F. and Handler, Alfred M. and Hahn, Daniel A.},
