}

@article{lundreganInfectionHelminthParasite2022,
  title = {Infection by a Helminth Parasite Is Associated with Changes in {{{\textsc{DNA}}}} Methylation in the House Sparrow},
  shorttitle = {Infection by a Helminth Parasite Is Associated with Changes In},
