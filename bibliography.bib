@article{vergeerEvidenceEpigeneticRole2012,
  title = {Evidence for an Epigenetic Role in Inbreeding Depression},
  author = {Vergeer, Philippine and Wagemaker, Niels (C. A. M.) and Ouborg, N. Joop},
