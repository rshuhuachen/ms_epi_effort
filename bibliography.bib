@article{rozempolska-rucinskaBehaviourStressThree2020,
  title = {Behaviour and Stress in Three Breeds of Laying Hens Kept in the Same Environment},
  author = {{Rozempolska-Ruci{\'n}ska}, I. and Czech, A. and Kasperek, K. and {Zi{\textbackslash}c eba}, G. and Ziemia{\'n}ska, A.},
  year = {2020},
  month = jul,
  journal = {South African Journal of Animal Science},
  volume = {50},
  number = {2},
  pages = {272--280},
  issn = {2221-4062},
  doi = {10.4314/sajas.v50i2.10},
  abstract = {The aim of the study was to determine whether birds of different breeds, with different preferences for environmental elements and kept in identical conditions, would exhibit different behaviour and stress levels. The research material consisted of 50 laying hens of the Green-legged partridge (Zk), Polbar (Pb), and Leghorn (Lg) breeds. The birds were provided with identical environmental conditions. They were kept on litter in separate boxes, with 25 individuals of one breed per box, in the same building. An open field test was used in determining the behaviour of the birds. Levels of cortisol in serum and corticosterone in feathers were evaluated, and haematological and biochemical analyses of blood were performed. Leghorns proved to be a highly excitable and active breed whose needs cannot be fully satisfied in the standard conditions of large farms. Based on a differential leukocyte count, their hyperactivity was associated with increased stress. The Green-legged partridge exhibited the lowest stress levels. Birds kept in the same environmental conditions were found to differ significantly in the level of stress, probably because of differences in their behavioural needs and the possibilities of satisfying them. To reduce stress levels and improve the welfare of hens, it is necessary to verify their preferences first. Stress indicators are elevated in highly excitable birds, and the conditions on large farms are not conducive to meeting their behavioural needs. Keywords: behavioural test, blood indices, corticosterone, laying hens},
  keywords = {/unread}
}

@article{ruddOvercomingRacismTwin2021,
  title = {Overcoming Racism in the Twin Spheres of Conservation Science and Practice},
  author = {Rudd, Lauren F. and Allred, Shorna and Bright Ross, Julius G. and Hare, Darragh and Nkomo, Merlyn Nomusa and Shanker, Kartik and Allen, Tanesha and Biggs, Duan and Dickman, Amy and Dunaway, Michael and Ghosh, Ritwick and Gonz{\'a}lez, Nicole Thompson and Kepe, Thembela and Mbizah, Moreangels M. and Middleton, Sara L. and Oommen, Meera Anna and Paudel, Kumar and {Sillero-Zubiri}, Claudio and D{\'a}valos, Andrea},
