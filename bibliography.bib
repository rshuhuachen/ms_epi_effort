@article{wangMajorLocusControls2018,
  title = {A Major Locus Controls Local Adaptation and Adaptive Life History Variation in a Perennial Plant},
  author = {Wang, Jing and Ding, Jihua and Tan, Biyue and Robinson, Kathryn M. and Michelson, Ingrid H. and Johansson, Anna and Nystedt, Bj{\"o}rn and Scofield, Douglas G. and Nilsson, Ove and Jansson, Stefan and Street, Nathaniel R. and Ingvarsson, P{\"a}r K.},
