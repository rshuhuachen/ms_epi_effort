@article{theodorouGenomewideSingleNucleotide2018,
  title = {Genome-Wide Single Nucleotide Polymorphism Scan Suggests Adaptation to Urbanization in an Important Pollinator, the Red-Tailed Bumblebee ( {{{\emph{Bombus}}}}{\emph{ Lapidarius}} {{L}}.)},
