@article{keightleyEvolutionaryConstraintsConserved2005,
  title = {Evolutionary Constraints in Conserved Nongenic Sequences of Mammals},
  author = {Keightley, Peter D. and Kryukov, Gregory V. and Sunyaev, Shamil and Halligan, Daniel L. and Gaffney, Daniel J.},
  year = {2005},
