@article{benitez-lopezImpactHuntingTropical2017,
  title = {The Impact of Hunting on Tropical Mammal and Bird Populations},
  author = {{Ben{\'i}tez-L{\'o}pez}, A. and Alkemade, R. and Schipper, A. M. and Ingram, D. J. and Verweij, P. A. and Eikelboom, J. A. J. and Huijbregts, M. A. J.},
