@article{garcia-doradoUnderstandingPredictingFitness2012,
  title = {Understanding and {{Predicting}} the {{Fitness Decline}} of {{Shrunk Populations}}: {{Inbreeding}}, {{Purging}}, {{Mutation}}, and {{Standard Selection}}},
  shorttitle = {Understanding and {{Predicting}} the {{Fitness Decline}} of {{Shrunk Populations}}},
