@article{cantarelMAKEREasyuseAnnotation2008,
  title = {{{MAKER}}: {{An}} Easy-to-Use Annotation Pipeline Designed for Emerging Model Organism Genomes},
  shorttitle = {{{MAKER}}},
  author = {Cantarel, Brandi L. and Korf, Ian and Robb, Sofia M.C. and Parra, Genis and Ross, Eric and Moore, Barry and Holt, Carson and S{\'a}nchez Alvarado, Alejandro and Yandell, Mark},
