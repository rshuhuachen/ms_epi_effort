@article{wangWholeGenomeSequencing2014,
  title = {Whole Genome Sequencing of the Black Grouse ({{Tetrao}} Tetrix): Reference Guided Assembly Suggests Faster-{{Z}} and {{MHC}} Evolution},
  shorttitle = {Whole Genome Sequencing of the Black Grouse ({{Tetrao}} Tetrix)},
  author = {Wang, Biao and Ekblom, Robert and Bunikis, Ignas and Siitari, Heli and H{\"o}glund, Jacob},
  year = {2014},
  journal = {BMC Genomics},
  volume = {15},
