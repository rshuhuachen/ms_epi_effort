  author = {Cockerill, Christopher A. and Hasselgren, Malin and Dussex, Nicolas and Dal{\'e}n, Love and {von Seth}, Johanna and Angerbj{\"o}rn, Anders and Wall{\'e}n, Johan F. and Landa, Arild and Eide, Nina E. and Flagstad, {\O}ystein and Ehrich, Dorothee and Sokolov, Aleksandr and Sokolova, Natalya and Nor{\'e}n, Karin},
  year = {2022},
  month = nov,
  journal = {Genes},
  volume = {13},
  number = {11},
  pages = {2124},
  issn = {2073-4425},
  doi = {10.3390/genes13112124},
  urldate = {2023-03-14},
  abstract = {Accelerating climate change is causing severe habitat fragmentation in the Arctic, threatening the persistence of many cold-adapted species. The Scandinavian arctic fox (Vulpes lagopus) is highly fragmented, with a once continuous, circumpolar distribution, it struggled to recover from a demographic bottleneck in the late 19th century. The future persistence of the entire Scandinavian population is highly dependent on the northernmost Fennoscandian subpopulations (Scandinavia and the Kola Peninsula), to provide a link to the viable Siberian population. By analyzing 43 arctic fox genomes, we quantified genomic variation and inbreeding in these populations. Signatures of genome erosion increased from Siberia to northern Sweden indicating a stepping-stone model of connectivity. In northern Fennoscandia, runs of homozygosity (ROH) were on average {\textasciitilde}1.47-fold longer than ROH found in Siberia, stretching almost entire scaffolds. Moreover, consistent with recent inbreeding, northern Fennoscandia harbored more homozygous deleterious mutations, whereas Siberia had more in heterozygous state. This study underlines the value of documenting genome erosion following population fragmentation to identify areas requiring conservation priority. With the increasing fragmentation and isolation of Arctic habitats due to global warming, understanding the genomic and demographic consequences is vital for maintaining evolutionary potential and preventing local extinctions.},
  langid = {english},
  keywords = {/unread},
  file = {/Users/vistor/Zotero/storage/3IGS6DS4/Cockerill et al_2022_Genomic Consequences of Fragmentation in the Endangered Fennoscandian Arctic.pdf}
}

