@article{sheldonBroodSizeInfluences2018,
  title = {Brood Size Influences Patterns of {{DNA}} Methylation in Wild {{Zebra Finches}} ({{Taeniopygia}} Guttata)},
  author = {Sheldon, Elizabeth L. and Schrey, Aaron W. and Ragsdale, Alexandria K. and Griffith, Simon C.},
  year = {2018},
