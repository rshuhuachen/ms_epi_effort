@article{bondelDistributionFitnessEffects2022,
  title = {The Distribution of Fitness Effects of Spontaneous Mutations in {{Chlamydomonas}} Reinhardtii Inferred Using Frequency Changes under Experimental Evolution},
  author = {B{\"o}ndel, Katharina B. and Samuels, Toby and Craig, Rory J. and Ness, Rob W. and Colegrave, Nick and Keightley, Peter D.},
