@article{duntschGenomicDataDifferent2021,
  title = {Genomic Data of Different Resolutions Reveal Consistent Inbreeding Estimates but Contrasting Homozygosity Landscapes for the Threatened {{Aotearoa New Zealand}} Hihi},
  author = {Duntsch, Laura and Whibley, Annabel and Brekke, Patricia and Ewen, John G. and Santure, Anna W.},
