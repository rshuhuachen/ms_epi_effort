  shorttitle = {Inbreeding and {{Loss}} of {{Genetic Variation}} in a {{Reintroduced Population}} of {{Mauritius Kestrel}}},
  author = {Ewing, Steven R. and Nager, Ruedi G. and Nicoll, Malcolm A. C. and Aumjaud, Aurelien and Jones, Carl G. and Keller, Lukas F.},
