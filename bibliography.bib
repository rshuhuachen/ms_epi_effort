@article{dugandMolecularEvidenceSupports2019,
  title = {Molecular Evidence Supports a Genic Capture Resolution of the Lek Paradox},
  author = {Dugand, Robert J. and Tomkins, Joseph L. and Kennington, W. Jason},
  year = {2019},
