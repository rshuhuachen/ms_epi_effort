@article{lariepeGeneticBasisHeterosis2012,
  title = {The {{Genetic Basis}} of {{Heterosis}}: {{Multiparental Quantitative Trait Loci Mapping Reveals Contrasted Levels}} of {{Apparent Overdominance Among Traits}} of {{Agronomical Interest}} in {{Maize}} ( {{{\emph{Zea}}}}{\emph{ Mays}} {{L}}.)},
  shorttitle = {The {{Genetic Basis}} of {{Heterosis}}},
  author = {Lari{\`e}pe, A and Mangin, B and Jasson, S and Combes, V and Dumas, F and Jamin, P and Lariagon, C and Jolivot, D and Madur, D and Fi{\'e}vet, J and Gallais, A and Dubreuil, P and Charcosset, A and Moreau, L},
