@article{marshallMethylationGeneExpression2019,
  title = {Methylation and Gene Expression Differences between Reproductive and Sterile Bumblebee Workers},
  author = {Marshall, Hollie and Lonsdale, Zo{\"e} N. and Mallon, Eamonn B.},
  year = {2019},
  month = oct,
