@article{milnerDemographicSideEffects2007a,
  title = {Demographic {{Side Effects}} of {{Selective Hunting}} in {{Ungulates}} and {{Carnivores}}},
  author = {Milner, Jos M. and Nilsen, Erlend B. and Andreassen, Harry P.},
