@article{caizerguesPopulationGeneticStructure2003,
  title = {Population Genetic Structure of Male Black Grouse ( {{{\emph{Tetrao}}}}{\emph{ Tetrix}} {{L}}.) in Fragmented vs. Continuous Landscapes},
