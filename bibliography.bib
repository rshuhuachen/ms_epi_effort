@article{larssonGeneticImpoverishmentLast2008,
  title = {Genetic Impoverishment of the Last Black Grouse ( {{{\emph{Tetrao}}}}{\emph{ Tetrix}} ) Population in the {{Netherlands}}: Detectable Only with a Reference from the Past: {{GENETIC DIVERSITY IN BLACK GROUSE}}},
  shorttitle = {Genetic Impoverishment of the Last Black Grouse ( {{{\emph{Tetrao}}}}{\emph{ Tetrix}} ) Population in the {{Netherlands}}},
  author = {Larsson, J. Karl and Jansman, Hugh A. H. and Segelbacher, Gernot and H{\"o}glund, Jacob and Koelewijn, Hans Peter},
  year = {2008},
