@article{kutscheraGenErodeBioinformaticsPipeline2022,
  title = {{{GenErode}}: A Bioinformatics Pipeline to Investigate Genome Erosion in Endangered and Extinct Species},
  shorttitle = {{{GenErode}}},
  author = {Kutschera, Verena E. and Kierczak, Marcin and {van der Valk}, Tom and {von Seth}, Johanna and Dussex, Nicolas and Lord, Edana and Dehasque, Marianne and Stanton, David W. G. and Khoonsari, Payam Emami and Nystedt, Bj{\"o}rn and Dal{\'e}n, Love and {D{\'i}ez-del-Molino}, David},
  year = {2022},
  month = dec,
  journal = {BMC Bioinformatics},
  volume = {23},
  number = {1},
  pages = {228},
  issn = {1471-2105},
  doi = {10.1186/s12859-022-04757-0},
  urldate = {2022-09-07},
  abstract = {Background:{\enspace} Many wild species have suffered drastic population size declines over the past centuries, which have led to `genomic erosion' processes characterized by reduced genetic diversity, increased inbreeding, and accumulation of harmful muta-tions. Yet, genomic erosion estimates of modern-day populations often lack concord-ance with dwindling population sizes and conservation status of threatened species. One way to directly quantify the genomic consequences of population declines is to compare genome-wide data from pre-decline museum samples and modern samples. However, doing so requires computational data processing and analysis tools specifi-cally adapted to comparative analyses of degraded, ancient or historical, DNA data with modern DNA data as well as personnel trained to perform such analyses. Results:{\enspace} Here, we present a highly flexible, scalable, and modular pipeline to compare patterns of genomic erosion using samples from disparate time periods. The GenErode pipeline uses state-of-the-art bioinformatics tools to simultaneously process wholegenome re-sequencing data from ancient/historical and modern samples, and to produce comparable estimates of several genomic erosion indices. No programming knowledge is required to run the pipeline and all bioinformatic steps are well-docu-mented, making the pipeline accessible to users with different backgrounds. GenErode is written in Snakemake and Python3 and uses Conda and Singularity containers to achieve reproducibility on high-performance compute clusters. The source code is freely available on GitHub (https://github.com/NBISweden/GenErode). Conclusions:{\enspace} GenErode is a user-friendly and reproducible pipeline that enables the standardization of genomic erosion indices from temporally sampled whole genome re-sequencing data.},
  langid = {english},
  keywords = {/unread,Methods},
  file = {/Users/vistor/Zotero/storage/QVKY7B9J/Kutschera et al. - 2022 - GenErode a bioinformatics pipeline to investigate.pdf}
}

