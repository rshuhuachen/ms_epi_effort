@article{ludwigShortLongtermPopulation2006,
  title = {Short- and Long-Term Population Dynamical Consequences of Asymmetric Climate Change in Black Grouse},
  author = {Ludwig, Gilbert X and Alatalo, Rauno V and Helle, Pekka and Lind{\'e}n, Harto and Lindstr{\"o}m, Jan and Siitari, Heli},
  year = {2006},
