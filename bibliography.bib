@article{somervilleDNAMethylationPatterns2019,
  title = {{{DNA Methylation Patterns}} in the {{Round Goby Hypothalamus Support}} an {{On-The-Spot Decision Scenario}} for {{Territorial Behavior}}},
  author = {Somerville, Vincent and Schwaiger, Michaela and Hirsch, Philipp E. and Walser, Jean-Claude and Bussmann, Karen and Weyrich, Alexandra and {Burkhardt-Holm}, Patricia and {Adrian-Kalchhauser}, Irene},
