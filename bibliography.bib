@article{luponWomeninclusiveEcologyRepresentation2021,
  title = {Towards Women-Inclusive Ecology: {{Representation}}, Behavior, and Perception of Women at an International Conference},
  shorttitle = {Towards Women-Inclusive Ecology},
  author = {Lupon, Anna and {Rodr{\'i}guez-Lozano}, Pablo and Bartrons, Mireia and {Anadon-Rosell}, Alba and Batalla, Meritxell and Bernal, Susana and Bravo, Andrea G. and Capdevila, Pol and {Ca{\~n}edo-Arg{\"u}elles}, Miguel and Catal{\'a}n, N{\'u}ria and {Genua-Olmedo}, Ana and {Guti{\'e}rrez-C{\'a}novas}, Cayetano and Feio, Maria Jo{\~a}o and Lucati, Federica and Onandia, Gabriela and Poblador, S{\'i}lvia and {Rotch{\'e}s-Ribalta}, Roser and {Sala-Bubar{\'e}}, Anna and {S{\'a}nchez-Montoya}, Mar{\'i}a Mar and Sebasti{\'a}n, Marta and Zufiaurre, Aitziber and Pastor, Ada},
  editor = {{Risse-Buhl}, Ute},
