@article{berdanHowChromosomalInversions2023,
  title = {How Chromosomal Inversions Reorient the Evolutionary Process},
  author = {Berdan, Emma L. and Barton, Nicholas H. and Butlin, Roger and Charlesworth, Brian and Faria, Rui and Fragata, In{\^e}s and Gilbert, Kimberly J. and Jay, Paul and Kapun, Martin and Lotterhos, Katie E. and M{\'e}rot, Claire and Durmaz~Mitchell, Esra and Pascual, Marta and Peichel, Catherine L. and Rafajlovi{\'c}, Marina and Westram, Anja M. and Schaeffer, Stephen W. and Johannesson, Kerstin and Flatt, Thomas},
  year = {2023},
