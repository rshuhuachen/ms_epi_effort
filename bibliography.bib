@article{szulkinInbreedingDepressionLifehistory2007,
  title = {Inbreeding Depression along a Life-history Continuum in the Great Tit},
  author = {Szulkin, M. and Garant, D. and Mccleery, R. H. and Sheldon, B. C.},
  year = {2007},
